{
  "name": "feldarmbrust-tablet-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "cap:init": "npx cap init FeldarmbrustApp at.feldarmbrust.app --web-dir=dist",
    "cap:add:android": "npm run build && npx cap add android",
    "cap:sync": "npm run build && npx cap sync android",
    "cap:open": "npx cap open android"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@capacitor/core": "^6.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.4.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/android": "^6.1.2"
  }
}
